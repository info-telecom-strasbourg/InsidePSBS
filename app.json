{
  "expo": {
    "scheme": "inside-psbs",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#000000",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ITS.InsidePSBS",
      "buildNumber": "2.1.6",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#081425"
      },
      "package": "com.ITS.InsidePSBS",
      "versionCode": 52,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "name": "InsidePSBS",
    "slug": "insidepsbs",
    "version": "2.1.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#081425"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you publish your avatar."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "61177b6a-7651-4fcf-8e36-b10e30374c5c"
      }
    },
    "owner": "info-telecom-strasbourg"
  }
}
