{
  "cli": {
    "version": ">= 16.10.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      },
      "autoIncrement": true
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "/tmp/google-service.json"
      },
      "ios": {
        "appleId": "info.telecom.strasbourg@gmail.com",
        "ascAppId": "6446891622",
        "appleTeamId": "2KPK2K7PUN"
      }
    }
  }
}
